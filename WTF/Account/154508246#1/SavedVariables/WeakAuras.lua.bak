						["custom"] = "function(allstates, e, arg1, arg2, unused_arg3, unused_arg4, unused_arg5, unused_arg6, unused_arg7, arg8)\n    if (e == \"UNIT_HEALTH\" and arg1 == \"target\" or e == \"PLAYER_TARGET_CHANGED\") and UnitExists(\"target\") and not UnitIsPlayer(\"target\") then\n        local _, _, _, _, _, npcid, unitid = strsplit(\"-\",UnitGUID(\"target\"))\n        if aura_env.mob_whitelist[tonumber(npcid)] then\n            C_ChatInfo.SendAddonMessage(\n                aura_env.prefix,\n                string.format(\"%s:%s:%s:%d:%d\",\n                    unitid,\n                    npcid,\n                    UnitName(\"target\"),\n                    UnitHealth(\"target\"),\n                    UnitHealthMax(\"target\")\n                ),\n                aura_env.channel\n            )\n        end\n    elseif e == \"UPDATE_MOUSEOVER_UNIT\" and UnitExists(\"mouseover\") and not UnitIsPlayer(\"mouseover\") then\n        local _, _, _, _, _, npcid, unitid = strsplit(\"-\",UnitGUID(\"mouseover\"))\n        if aura_env.mob_whitelist[tonumber(npcid)] then\n            C_ChatInfo.SendAddonMessage(\n                aura_env.prefix,\n                string.format(\"%s:%s:%s:%d:%d\",\n                    unitid,\n                    npcid,\n                    UnitName(\"mouseover\"),\n                    UnitHealth(\"mouseover\"),\n                    UnitHealthMax(\"mouseover\")\n                ),\n                aura_env.channel\n            )\n        end\n    elseif e == \"CHAT_MSG_ADDON\" then\n        local pfx, msg = arg1, arg2\n        \n        if pfx ~= aura_env.prefix then return false end\n        if aura_env.debug then\n            print(e, arg1, arg2, arg8)\n        end\n        \n        local unitid, npcid, name, current, max = strsplit(\":\", msg)\n        current = tonumber(current)\n        max = tonumber(max)\n        \n        if not allstates[unitid] then \n            allstates[unitid] = {\n                show = true,\n                progressType = \"static\",\n                total = 100,\n                index = name .. unitid,\n                npcid = npcid,\n                name = name,\n                lastUpdate = time(),\n            }\n        end\n        \n        allstates[unitid].value = math.floor(current / max * 100)\n        allstates[unitid].changed = true\n        allstates[unitid].lastUpdate = time()\n        local allDead = true\n        for _, state in pairs(allstates) do\n            allDead = allDead and state.value == 0\n        end\n        for unitid, state in pairs(allstates) do\n            if allDead then\n                state.show = false\n            elseif state.npcid ~= \"15802\" and time() - state.lastUpdate > 30 then\n                state.show = false\n            elseif state.npcid == \"15802\" and time() - state.lastUpdate > 60 then\n                state.show = false\n            elseif state.npcid ~= \"15802\" and state.value == 0 then\n                state.show = false\n            end\n        end\n        return true\n    elseif e == \"COMBAT_LOG_EVENT_UNFILTERED\" and arg2 == \"UNIT_DIED\" then\n        local _, _, _, _, _, npcid, unitid = strsplit(\"-\",arg8)\n        if allstates[unitid] then\n            C_ChatInfo.SendAddonMessage(\n                aura_env.prefix,\n                string.format(\"%s:%s:%s:%d:%d\",\n                    unitid,\n                    npcid,\n                    \"\",\n                    0,\n                    100\n                ),\n                aura_env.channel\n            )\n        end  \n    end\nend",
